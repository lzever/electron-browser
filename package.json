{
  "name": "lovense_browser",
  "version": "1.1.0",
  "description": "An application browser",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node --max-old-space-size=5120 .electron-vue/build.js && electron-builder",
    "build:dir": "node --max-old-space-size=5120 .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "watch": "cross-env NODE_ENV=develop webpack --watch --progress --colors --config .electron-vue/watch-renderer.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "pack:guest": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.guest.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix",
    "exe": "electron-builder",
    "exe-dev": "electron dist/electron/main.js"
  },
  "homepage": "",
  "repository": "",
  "author": {
    "name": "",
    "email": ""
  },
  "license": "MIT",
  "build": {
    "productName": "Lovense Browser",
    "appId": "com.lovense-browser.app",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "extraResources": "./lib/**/*",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true
    },
    "linux": {
      "icon": "build/icons/icon.ico",
      "target": [
        {
          "target": "tar.gz",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.11",
    "autoprefixer": "^9.1.5",
    "axios": "^0.18.0",
    "blueimp-md5": "^2.10.0",
    "body-parser": "^1.18.3",
    "cssnano": "^4.1.4",
    "dotenv": "^4.0.0",
    "element-ui": "^2.4.9",
    "encoding": "^0.1.2",
    "jquery": "^1.12.4",
    "js-cookie": "^2.2.0",
    "loadenvjs": "^1.0.1",
    "moment": "^2.22.1",
    "nedb": "^1.8.0",
    "node-cmd": "^3.0.0",
    "qs": "^6.5.2",
    "sortablejs": "^1.7.0",
    "tldjs": "^2.3.1",
    "uuid": "^3.2.1",
    "vue": "^2.5.11",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vuedraggable": "^2.16.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "autotrack": "^2.4.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "backoff": "^2.5.0",
    "bufferutil": "^4.0.1",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "copy-webpack-plugin": "^4.0.1",
    "crawler": "^1.1.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^9.4.0",
    "electron-builder": "^19.49.0",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.2",
    "encoding": "0.1.2",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "file-saver": "^1.3.8",
    "hiredis": "^0.5.0",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-abi": "^2.5.1",
    "node-fs-extra": "^0.8.2",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.0",
    "node-schedule": "^1.3.0",
    "object-assign": "^4.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "0.0.3",
    "postcss-viewport-units": "^0.1.4",
    "rasterizehtml": "^1.3.0",
    "require-dir": "^0.3.0",
    "sass-loader": "^7.1.0",
    "socket.io": "^2.1.1",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "superagent": "^2.0.0",
    "url-loader": "^0.5.9",
    "utf-8-validate": "^4.0.2",
    "vee-validate": "^2.1.0-beta.8",
    "vue-html-loader": "^1.2.4",
    "vue-i18n": "^7.6.0",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.11",
    "vuelidate": "^0.7.4",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "ws": "^3.0.0"
  }
}
